{
  "name": "SaveState",
  "version": "1.0",
  "description": "This extension saves all tab URL's in current windows. The URL's can then be reloaded again",
  "permissions": ["activeTab", "declarativeContent", "storage","tabs"],
  "background": {
   "scripts": ["background.js"]
},
  "browser_action": {
    "default_popup": "extension.html",
    "default_icon": {
      "16": "images/SS_icon.ico",
      "32": "images/SS_icon.ico",
      "48": "images/SS_icon.ico",
      "128": "images/SS_icon.ico"
    }
  },
  "manifest_version": 2
}
